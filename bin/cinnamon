#!/usr/bin/env perl
use strict;
use warnings;
use File::Basename;
BEGIN {
    my $file_name = dirname(__FILE__) . q</../config/perl/libs.txt>;
    if (-f $file_name) {
        open my $file, '<', $file_name or die "$0: $file_name: $!";
        unshift @INC, split /:/, <$file>;
    }
}
use Cinnamon;

Cinnamon->run(@ARGV);
